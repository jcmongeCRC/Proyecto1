          SQLTask:Connection="{4C14242E-6FB6-4E85-88D9-4E9DC24BC057}"
          SQLTask:SqlStatementSource="DECLARE @PRODUCTO AS INT;&#xA;DECLARE @COLOR AS NVARCHAR(15);&#xA;DECLARE @PRECIO AS MONEY;&#xA;DECLARE @TAMANO AS NVARCHAR(5);&#xA;DECLARE @NUEVOTAMANO AS NVARCHAR(5);&#xA;DECLARE @RESULTADO AS VARCHAR(255);&#xA;&#xA;SET @PRODUCTO = ?;&#xA;SET @COLOR = ?;&#xA;SET @PRECIO = ?;&#xA;SET @TAMANO = ?;&#xA;&#xA;IF @TAMANO = 'S' SET @TAMANO = 'A'&#xA;IF @TAMANO = 'M' SET @TAMANO = 'B'&#xA;IF @TAMANO = 'L' SET @TAMANO = 'C'&#xA;IF @TAMANO = 'XL' SET @TAMANO = 'D'&#xA;&#xA;&#xA;SELECT TOP 1 @NUEVOTAMANO = (CASE WHEN ISNULL(SIZE,'ND') = 'S' THEN 'A' WHEN ISNULL(SIZE,'ND') = 'M' THEN 'B' &#xA;&#x9;WHEN ISNULL(SIZE,'ND') = 'L' THEN 'C' WHEN ISNULL(SIZE,'ND') = 'XL' THEN 'D' ELSE ISNULL(SIZE,'ND') END), &#xA;&#x9;@RESULTADO = 'Para el producto indicado  ' + CAST(@PRODUCTO AS VARCHAR(10)) + ' ' + [NAME] + &#xA;&#x9;' la variable de precio ' +&#xA;&#x9;CASE WHEN @PRECIO = ListPrice THEN ' IGUAL ' WHEN @PRECIO &lt; ISNULL(ListPrice,0) THEN ' MENOR ' ELSE ' MAYOR ' END + ', ' +&#xA;&#x9;' la variable de tamaño ' +&#xA;&#x9;CASE WHEN @TAMANO = 'E' THEN ' DIFERENTE ' WHEN @TAMANO &lt; @NUEVOTAMANO&#x9;THEN ' MENOR ' WHEN @TAMANO &gt; @NUEVOTAMANO THEN ' MAYOR ' &#xA;&#x9;WHEN @TAMANO = @NUEVOTAMANO THEN ' IGUAL ' &#xA;&#x9;END + ', ' +&#xA;&#x9;' la variable de color ' +&#xA;&#x9;CASE WHEN @COLOR = ISNULL(Color,'ND') THEN ' IGUAL ' ELSE ' DIFERENTE ' END + '.' &#xA;FROM SalesLT.Product&#xA;WHERE ProductID = @PRODUCTO&#xA;&#xA;&#xA;SELECT ISNULL(@RESULTADO, 'El producto no existe') AS RESULTADO"
